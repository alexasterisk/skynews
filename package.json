{
    "name": "skynews",
    "version": "0.0.0",
    "type": "module",
    "description": "A bot for the Skyblock API",
    "exports": "./dist/index.js",
    "scripts": {
        "check:lint": "eslint src --ext .ts",
        "check:format": "prettier --check src",
        "check": "npm run check:lint && npm run check:format",
        "format": "prettier --write src",
        "lint": "npm run check:lint -- --fix",
        "build": "tsc --build --verbose",
        "deploy": "npm run build && node dist/index.js",
        "dev": "nodemon --watch src --exec \"ts-node src/index.ts\"",
        "pm2": "npm run build && pm2 start dist/index.js --name \"skynews\""
    },
    "dependencies": {
        "@keyv/sqlite": "^3.6.5",
        "@made-simple/discord.js": "^3.1.2",
        "@types/cron": "^2.0.1",
        "chalk": "^5.2.0",
        "cron": "^2.3.1",
        "discord.js": "^14.11.0",
        "dotenv": "^16.1.4",
        "keyv": "^4.5.2",
        "node-fetch": "^3.3.1"
    },
    "devDependencies": {
        "@types/node": "^20.2.6",
        "@typescript-eslint/eslint-plugin": "^5.59.9",
        "@typescript-eslint/parser": "^5.59.9",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "nodemon": "^2.0.22",
        "pm2": "^5.3.0",
        "prettier": "^2.8.8",
        "ts-node": "^10.9.1",
        "tslib": "^2.5.3",
        "typescript": "^5.1.3"
    },
    "engines": {
        "node": ">=18.12"
    }
}
